<studierstube>
	<kernel 
	    logMode="off" 
		logFileName="kernelLog.txt" 
		guibinding="sowin"
		updateMode="timer"
		updateRate="0.03333"/>
	
    <Component  name="Event"
		        availability="ondemand"	
        	    lib="stbevent">
	<Param key   ="configFile" 
		   value ="opentracker.xml" />	
	</Component>
	

	<Component  name="Starlight"
	            availability="ondemand"	
        	    lib="starlight">
	</Component>
	
	<Component  name="Viewer"
	            availability="ondemand"	
                lib="stbviewer">
	    <Param  key   ="configFile" 
		        value ="viewer.iv" />	
	</Component>


	
	<Component  name="Video"
	            availability="ondemand"	
        	    lib="stbvideo">
	    <Param key   ="configFile" 
		        value ="openvideo.xml" />	
	</Component>
	
	
    <Application name="SimpleApp"
                availability="onload"
                lib="applications/simple">     
                <Param  key   ="sceneFile"  value ="applications/scene.iv" />	
                <Param  key   ="needViewer" value ="true" />	
                <Param  key   ="needEvent"  value ="true" />	
		               


    </Application>
    

</studierstube>