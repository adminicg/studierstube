<studierstube>
	<kernel 
	    logMode="off" 
		logFileName="kernelLog.txt" 
		guibinding="sowin"
		updateMode="timer"
		updateRate="0.03333"/>
	
    <Component  name="Event"
		        availability="ondemand"	
        	    lib="stbevent">
	<Param key   ="configFile" 
		   value ="opentracker.xml" />	
	</Component>
	

		<Component  name="Viewer"
	        availability="onload"	
        	lib="stbviewer">
	    <Param key   ="configFile" 
		       value ="viewer.iv" />	
	</Component>


	
	<Component  name="Video"
	            availability="ondemand"	
        	    lib="stbvideo">
	    <Param key   ="configFile" 
		        value ="openvideo.xml" />	
	</Component>
	
	
    <Application name="SimpleApp"
                availability="onload"
                lib="simple"
    />
    

</studierstube>