<studierstube>
	<kernel 
	    logMode="off" 
		logFileName="kernelLog.txt" 
		guibinding="sowin"
		updateMode="timer"
		updateRate="0.03333"/>
	
		<Application 
	        name="SimpleApp"
		    availablity="onload"
		    lib="simple"	
	    />
	<Component  name="Viewer"
	        availability="ondemand"	
        	lib="stbviewer">
	    <Param key   ="configFile" 
		       value ="viewer.iv" />	
	</Component>
	
	<Component  name="Event"
		        availability="ondemand"	
        	    lib="stbevent">
	<Param key   ="configFile" 
		   value ="opentracker.xml" />	
	</Component>


	

</studierstube>