<?xml version="1.0" encoding="UTF-8"?>
<OpenTracker>
	<configuration>
		<!-- Here we place all the necessary configurations -->
	    <ConsoleConfig headerline="Sample Tracking Input" interval="1" display="on"/>
	    <EventConfig keyevents="on" mouseevents="on"/>
	</configuration>

	<!-- With a console sink we send the output to the shell -->	
	<ConsoleSink comment="Test Source" active="off">
		<!-- This sink is in charge of sending data to stb4 -->
		<EventSink tracking="otTrack">
			<!-- A test source generating mostly noise -->
			<EventUtilityNode create="string trackerid keyboard">
			<EventUtilityNode create="int intattr -10000">
			<EventUtilityNode create="float floatattr 282.3543">
			<EventUtilityNode create="unsigned_int uintattr 8888">
			<EventUtilityNode create="unsigned_short ushortattr 9">
			<EventUtilityNode create="unsigned_long ulongattr 91">
			<EventUtilityNode create="short shortattr -900"> 
			<EventUtilityNode create="bool boolattr 1">
		<!--	<EventUtilityNode create="vector<float> vec3fattr 1 0.2 89.5">
		<EventUtilityNode create="vector<float> rotfattr 1 0.2 89.5
		99,2"> -->



			<TestSource position="1 1 1" noise="0.5"
	frequency="10"/> 

<!--	   </EventUtilityNode> 
	   </EventUtilityNode> -->
	   </EventUtilityNode> 
	   </EventUtilityNode> 
	   </EventUtilityNode> 
	   </EventUtilityNode> 
	   </EventUtilityNode> 
	   </EventUtilityNode>
	   </EventUtilityNode> 
	   </EventUtilityNode>

		</EventSink>
	</ConsoleSink>

</OpenTracker>
